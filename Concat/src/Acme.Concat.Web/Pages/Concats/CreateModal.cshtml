@page
@using Acme.Concat.Localization
@using Acme.BookStore.Web.Pages.Concats
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Form
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<ConcatResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Concat" asp-page="/Concats/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewEmploye"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>